{% extends 'ArcherMeetEntryBundle::layout.html.twig' %}

{% block body -%}
    <h1>Entry</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Raw_seed</th>
                <td>{{ entity.rawseed }}</td>
            </tr>
            <tr>
                <th>Seed_time_seconds</th>
                <td>{{ entity.seedtimeseconds }}</td>
            </tr>
            <tr>
                <th>Seed_mark_centimeters</th>
                <td>{{ entity.seedmarkcentimeters }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('entry') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('entry_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
